@using SupperSlonicDomain.Models.SourceCodeDownload;

@model IList<DownloadInfo>

@{
    ViewBag.Title = "SupperSlonic Downloads Info";
}
<link href="~/Content/css/downloadsInfo.css" rel="stylesheet" type="text/css" />
<h1 class="first">Download information</h1>
<div class="downloadsInfo">
    <div class="downloadsInfoHeader">&nbsp;</div>

    @foreach (DownloadInfo downloadInfo in this.Model)
        { @Html.Partial("DownloadInfo", downloadInfo) }
</div>